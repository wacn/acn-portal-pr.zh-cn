<script type="text/javascript">
      
      
         function getUrlParam(name) {
          var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)");
          var r = window.location.search.substr(1).match(reg);
          var tempStr = "";
          if (r != null) {
              tempStr = decodeURIComponent(r[2]);
          } else {
              tempStr = "-";
          }
          return tempStr;
      };
      $("._banner-h2 span").html(returnErrorMessage(getUrlParam("error")));
      
      
        function returnErrorMessage(str){
            var regSub = /\u91cd\u590d\u63d0\u4ea4/
            if( str.indexOf("The same ticket already exists") != -1 ){
                return str
            }else if(regSub.test(str)){
                return "Duplicated submission"
            }else{
                return "Other exception"
            }
            
        }
       
       
  </script>
