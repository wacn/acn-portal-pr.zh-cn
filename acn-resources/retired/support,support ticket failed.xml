<script type="text/javascript">
      
      
         function getUrlParam(name) {
          var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)");
          var r = window.location.search.substr(1).match(reg);
          var tempStr = "";
          if (r != null) {
              tempStr = decodeURIComponent(r[2]);
          } else {
              tempStr = "-";
          }
          return tempStr;
      };
      $("._banner-h2 span").html(returnErrorMessage(getUrlParam("error")));
      
      
        function returnErrorMessage(str){
            var regSub = /\u91cd\u590d\u63d0\u4ea4/
            if( str.indexOf("已存在相同工单") != -1 ){
                return str
            }else if(regSub.test(str)){
                return "重复提交"
            }else{
                return "其他异常"
            }
            
        }
       
       
  </script>
