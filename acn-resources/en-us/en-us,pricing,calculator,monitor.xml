            <div class="common-banner col-top-banner" data-config="{'backgroundColor':'#e3f4ff', 'backgroundImage':'//resource.cdn.azure.cn/marketing-resource/css/images/service/product_banner_monitor.png','imageHeight':'auto'}">
                <div class="common-banner-image">
                    <div class="common-banner-title">
                        <svg id="Layer_1" data-name="Layer 1" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 48 48" width="48" height="48" style="float:left;"><defs><style>.azure-monitor-1{fill:#fff;}.azure-monitor-2,.azure-monitor-3,.azure-monitor-4,.azure-monitor-5,.azure-monitor-8{fill:#7fba00;}.azure-monitor-2{opacity:0.4;}.azure-monitor-2,.azure-monitor-3,.azure-monitor-4,.azure-monitor-5{isolation:isolate;}.azure-monitor-3{opacity:0.8;}.azure-monitor-4{opacity:0.6;}.azure-monitor-5{opacity:0.25;}.azure-monitor-6{fill:#a0a1a2;}.azure-monitor-7{fill:#b3b4b5;}.azure-monitor-9{fill:#dd5900;}.azure-monitor-10{fill:#ff8c00;}</style></defs><title>icons-azure</title><path class="azure-monitor-1" d="M23.26,8.5A16.6,16.6,0,1,0,35.33,12,16.3,16.3,0,0,0,25,8.4a17.13,17.13,0,0,0-1.78.09"/><path class="azure-monitor-2" d="M18.17,20.58l-4-3.27a13.3,13.3,0,0,0-2.39,8.19L16.9,25a8.16,8.16,0,0,1,1.27-4.37"/><path class="azure-monitor-3" d="M29.46,18l3.27-4a13.31,13.31,0,0,0-8.19-2.39l.54,5.16A8.17,8.17,0,0,1,29.46,18"/><path class="azure-monitor-4" d="M23.28,17l-.54-5.17a13.32,13.32,0,0,0-7.48,4.1l4,3.27a8.15,8.15,0,0,1,4-2.2"/><path class="azure-monitor-5" d="M17.1,26.75l-5.17.55A13.31,13.31,0,0,0,16,34.78l3.27-4a8.15,8.15,0,0,1-2.2-4"/><path class="azure-monitor-6" d="M44.89,22.91A20.1,20.1,0,0,0,37.47,9.37,19.39,19.39,0,0,0,35,7.69l-1.57,3A15.85,15.85,0,0,1,35.33,12a16.59,16.59,0,0,1-8.59,29.5,16.49,16.49,0,0,1-8.6-1.41l-1.57,3A20,20,0,0,0,44.89,22.91"/><path class="azure-monitor-6" d="M14.56,37.9A16.59,16.59,0,0,1,33.42,10.71l1.57-3A20,20,0,0,0,9.46,12.42h0a19.93,19.93,0,0,0,7.12,30.69l1.57-3a16.65,16.65,0,0,1-3.58-2.2"/><path class="azure-monitor-7" d="M14.56,37.9A16.59,16.59,0,0,1,33.42,10.71l1.57-3A20,20,0,0,0,9.46,12.42h0a19.93,19.93,0,0,0,7.12,30.69l1.57-3a16.65,16.65,0,0,1-3.58-2.2"/><path class="azure-monitor-8" d="M34.14,15.12l-3.27,4a8.13,8.13,0,0,1,1.81,2.75h5.42A13.3,13.3,0,0,0,34.14,15.12Z"/><path class="azure-monitor-9" d="M23.47,26.63a2.34,2.34,0,0,1,1.65-4,2.3,2.3,0,0,1,1.65.65H38.7a1.7,1.7,0,1,1,0,3.4H26.65a2.38,2.38,0,0,1-1.54.62,2.32,2.32,0,0,1-1.64-.68"/><path class="azure-monitor-10" d="M38.7,23.3H26.76a2.3,2.3,0,0,0-1.65-.65A2.34,2.34,0,0,0,22.77,25H40.41A1.7,1.7,0,0,0,38.7,23.3Z"/></svg>
                        <h2>Azure Monitor</h2>
                        <h4>Highly granular and real-time monitoring data for any Azure resource</h4>
                        <em><a href="/pricing/1rmb-trial/" id="home_monitor_trial">Apply for 1RMB Trial<i class="icon icon-arrow-right-thin"></i></a></em> <a href="/pricing/pia/" id="home_monitor_pia">Buy Now<i class="icon icon-arrow-right-thin"></i></a>
                    </div>
                </div>
            </div>
            <!-- END: Product-Detail-TopBanner --> <!-- BEGIN: Pricing calculator -->
            <div class="pricing-page-section">                      
                <div id="calculator" data-service="azure-monitor data-transferenter data-transfer support" data-addService=""></div>

                <!-- BEGIN: Pricing calculator Sidebar-->                      
                <div class="sidebar-win calculator empty hide">
                    <div class="control">
                        <a href="#" class="close" id="closeCalculator"></a>
                    </div>
                    <ul class="calcul-list">
                        <span class="nothing">You haven’t yet selected your price estimation service. Please add it by adjusting the amount on the left-hand side.</span>
                    </ul>
                    <div class="summary">
                        <div class="total">
                            Total price
                            <p>
                                <span class="num">RMB0.00</span> <span class="unit">/month</span>
                            </p>
                        </div>
                        <div class="btn-group">
                            <a class="calcul-btn buy" href="/pricing/pia/" id="sidebarWinCalculBuy">Go to purchase</a> <a class="calcul-btn-white export" href="#" id="sidebarWinCalculExport">Export list</a> <a class="calcul-btn-white reset" href="#" id="sidebarWinCalculReset">Reset</a>
                        </div>
                        <p class="summary-text">
                            Still not convinced? <a href="/pricing/1rmb-trial/" id="sidebarWinCalculTry">Try</a> these features first.
                        </p>
                    </div>
                </div>
             
                <!-- start calculator modal -->
                <div class="preload">
                    <img src="//resource.cdn.azure.cn/marketing-resource/css/img/right.png">
                </div>
                <div class="modal modal-calculator hide">
                    <div class="container">
                        <a href="#" class="close" id="closeCalculatorModal"></a>
                        <p>We will add service items selected on different pages to the combined calculation in the right-hand list. You can manually delete or clear items.</p>
                        <span class="checkbox-rect"></span>Don’t prompt again <a class="btn btn-continue" href="#">I’ve got it</a>
                    </div>
                </div>
                <!-- end calculator modal --> <!-- start calculator shopinglist template -->
                <script type="text/template" id="templPricingShopingList">
                    <% _.each(shopingList, function(item){%>
                    <li class="calcul-item">
                        <img src="<%= item.logoUrl %>">
                        <a class="delete" href="#"></a>
                        <div class="info">
                            <div class="name">
                                <span class="short" title="<%= item.name %>"><%= item.name %></span>
                                <% if(item.name.length > 10) { %>
                                <span>...</span>
                                <% } %>
                            </div>
                            <span class="type">
                                <% _.each(item.details, function(detail){ %>
                                <%= detail  %>&nbsp;&nbsp;
                                <% }) %>
                            </span>
                        </div>
                        <div class="time">
                            <%= item.number %><%= item.unit %>
                        </div>
                        <div class="detail">
                            <div class="price">
                                <span class="num">￥<%= item.currentMoney %></span>
                                <span class="unit">/月</span>
                            </div>
                        </div>
                    </li>
                    <% }) %>
                    <% if(shopingList.length == 0){ %>
                    <span class="nothing">您还没有选择要估算价格的服务，调整左边数量进行添加</span>
                    <% } %>
                </script>
                <!-- end calculator shopinglist template --> <!-- start calculator animation layer -->
                <div class="anima-calculator hide">
                    +1
                </div>
                <!-- end calculator animation layer -->
                <div class="azsidebar"></div>  
                <!-- END: Pricing calculator Sidebar-->                  
            </div>
            <!-- END: Pricing calculator --> <!--BEGIN: Support and service code chunk--> <?UMBRACO_MACRO chunkpath="production" chunkname="production-bottom-banner" macroAlias="blankCodeChunk" />  <!--END: Support and service code chunk-->                
    
    ]]></bodyText>
